extends layout

block content
  h1 NYT Article Search
  form(action='/' method='post')
    input(type='submit' value='Search' id='button')
    input(type='text' name='search' id='input')
  
  
  if res
    div(id='results')
      each val in res
        p(id='title')
          a(href=val.web_url)= val.snippet
        
        //- iframe(src=val.web_url id='frame' height='300' width='800')

  script(src='/javascripts/dom.js')
